{
  "name": "post-rate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node build/index.js",
    "predev": "npm run swagger",
    "prebuild": "npm run swagger",
    "build": "tsc",
    "dev": "cross-env NODE_ENV=development && concurrently \"nodemon\" \"nodemon -x tsoa spec\"",
    "swagger": "tsoa spec",
    "test": "jest",
    "migrate": "./node_modules/.bin/sequelize db:migrate",
    "create:db": "./node_modules/.bin/sequelize db:create",
    "make:migration": "./node_modules/.bin/sequelize migration:generate --name",
    "undo:migrate": "./node_modules/.bin/sequelize db:migrate:undo",
    "undo:migrate:all": "./node_modules/.bin/sequelize db:migrate:undo:all",
    "seed": "./node_modules/.bin/sequelize db:seed:all",
    "make:seeder": "./node_modules/.bin/sequelize seed:generate --name",
    "undo:seeder": "./node_modules/.bin/sequelize db:seed:undo",
    "undo:seeder:all": "./node_modules/.bin/sequelize db:seed:undo:all",
    "lint:init": "tslint --init",
    "lint": "tslint --project tsconfig.json",
    "lint:fix": "tslint --project tsconfig.json --fix"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.8",
    "@types/express-boom": "^3.0.0",
    "@types/faker": "^5.1.5",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.17.7",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/validator": "^13.6.3",
    "concurrently": "^5.3.0",
    "faker": "^5.1.0",
    "jest": "^26.6.3",
    "sequelize-cli": "^6.2.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^10.1.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-boom": "^3.0.0",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.5.0",
    "swagger-ui-express": "^4.1.4",
    "tsoa": "^3.8.0"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node src/index.ts"
  }
}
